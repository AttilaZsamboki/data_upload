	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Django ASGI (Daphne)",
			"type": "python",
			"request": "launch",
			"module": "${workspaceFolder}/manage.py",
			"args": [
				"runserver",
				"--no-color",
				"--noreload",
				"--nothreading",
				"--port",
				"8001",
				"--application",
				"dataupload.asgi:application"
			],
			"django": true,
			"justMyCode": true
		}
	]
}
